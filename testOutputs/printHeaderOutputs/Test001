struct __A;
struct __A_VT;

typedef __A* A;

struct __A
{
   __A_VT* __vtpr;

   __A();

   static String toString()
   static Class __class();

   static __A_VT  __vtable;
};

struct __A_VT
{
   Class __isa;
   String (*toString)(A);

   __A_VT()
    :__isa(__A::__class()),
     toString(&__A::toString),
    {}
};